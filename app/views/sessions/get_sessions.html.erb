
<!-- Right side column. Contains the navbar and content of the page -->
        <aside class="right-side">
            <!-- Content Header (Page header) -->
            <section class="content-header">
                <h1>Sessions Data<small></small> </h1>
                <ol class="breadcrumb">
                    <li><a href="/"><i class="fa fa-home"></i>Home</a></li>
                    <li>Sessions</li>
                    <li class="active">Sessions Data</li>
                </ol>
            </section>
            
<!-- Main content -->
            <section class="content">
                <div class="row">
                   
                    <br />
                    <br />
                    <div class="col-xs-12">
                        <div class="box">
                           
						   <!--<div class="box-header">
							<h3 class="box-title">Data Table With Full Features</h3>
							</div>-->
                            <!-- /.box-header -->
							
							
                            <div class="box-body table-responsive">
                                <%= session.keys.each do |cur_session_key| %>
                                	<label>session[:<%= cur_session_key %>] = <%= session[cur_session_key] %></label><br/>
                                <% end %>
                            </div>
                            <!-- /.box-body -->
                        </div>
                        ID: <%= @employee.id.to_s %></br>
                        name   <%= @employee.name.to_s %></br>                        
                        fte   <%= @employee.fte.to_s %></br>
                        last updated   <%= @employee.updated_at.to_s %></br>
                        
                        <!-- /.box -->
                    </div>
		<div>

                    <% if @exams.size > 0 %>
                      <%= harbinger_table_for @exams, :class => "table table-striped" do |ht| %>
                        <% ht.column("Accession") {|e| e.accession  if e.accession } %>        
                          <% ht.column("MRN") {|e| e.mrn if e.mrn } %>          
                        <% ht.column("Current Status") {|e| e.current_status if e.current_status} %>
                          <% ht.column("Procedure Code") {|e| e.procedure_id if e.procedure_id } %>  
                     <% ht.column("proc description") {|e| e.description if e.description } %>      
                    <% ht.column(" ordering provider") {|e| e.ordering_provider if e.ordering_provider } %>      
                    <% ht.column("scheduler") {|e| e.scheduler if e.scheduler } %>      
                    <% ht.column("technologist") {|e| e.technologist if e.technologist } %>      
                    
                        <% end %>
                      <% else %>
                        No exams.
                      <% end %>
                </div>
                    <div>
                        <%= @exams.to_json %>
                    </div>
                </div>



            </section>
